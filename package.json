{
    "name": "galaxy-charts",
    "description": "Galaxy Charts",
    "keywords": [
        "galaxy",
        "charts"
    ],
    "license": "MIT",
    "version": "0.0.32",
    "type": "module",
    "main": "./dist/galaxy-charts.umd.cjs",
    "module": "./dist/galaxy-charts.js",
    "types": "./dist/types.d.ts",
    "exports": {
        ".": {
            "import": "./dist/galaxy-charts.js",
            "require": "./dist/galaxy-charts.js",
            "types": "./dist/types.d.ts"
        }
    },
    "files": [
        "dist/galaxy-charts.css",
        "dist/galaxy-charts.js",
        "dist/galaxy-charts.umd.cjs",
        "dist/types.d.ts"
    ],
    "scripts": {
        "dev": "npm run build && vite",
        "build": "vite build",
        "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
        "postbuild": "npm run build:types",
        "preview": "vite preview",
        "prettier": "prettier --write 'package.json' '*.js' 'src/**/*.js' 'src/**/*.vue'",
        "test": "vitest"
    },
    "devDependencies": {
        "@heroicons/vue": "^2.1.5",
        "@vitejs/plugin-vue": "^5.0.5",
        "@vitest/ui": "^2.1.2",
        "@vue/test-utils": "^2.4.6",
        "autoprefixer": "^10.4.19",
        "axios": "^1.7.7",
        "galaxy-charts-xml-parser": "^1.0.3",
        "jsdom": "^25.0.1",
        "naive-ui": "^2.39.0",
        "postcss": "^8.4.40",
        "prettier": "^3.3.3",
        "tailwindcss": "^3.4.7",
        "typescript": "^5.5.4",
        "vite": "^5.3.4",
        "vite-plugin-checker": "^0.8.0",
        "vite-plugin-lib-inject-css": "^2.1.1",
        "vitepress": "^1.3.3",
        "vitest": "^2.1.2",
        "vue": "^3.4.31",
        "vue-tsc": "^2.1.10"
    }
}
